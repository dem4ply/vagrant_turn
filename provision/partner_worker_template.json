{
  "index_patterns": [
    "*__turn_api__partner_worker"
  ],
  "mappings": {
    "doc": {
      "properties": {
        "id": {
          "type": "integer"
        },
        "partner": {
          "properties": {
            "name": {
              "fields": {
                "raw": {
                  "type": "keyword"
                }
              },
              "type": "text"
            },
            "id": {
              "type": "integer"
            },
            "type": {
              "type": "keyword"
            }
          },
          "type": "object"
        },
        "first_name": {
          "analyzer": "person_name",
          "fields": {
            "raw": {
              "type": "keyword"
            }
          },
          "type": "text"
        },
        "last_name": {
          "analyzer": "person_name",
          "fields": {
            "raw": {
              "type": "keyword"
            }
          },
          "type": "text"
        },
        "email": {
          "fields": {
            "raw": {
              "type": "keyword"
            }
          },
          "type": "text"
        },
        "profile_status": {
          "type": "keyword"
        },
        "uuid": {
          "type": "keyword"
        },
        "create_at": {
          "type": "date"
        }
      }
    }
  },
  "settings": {
    "analysis": {
      "analyzer": {
        "person_name": {
          "tokenizer": "trigram",
          "filter": [
            "standard",
            "lowercase",
            "asciifolding",
            "stop",
            "snowball"
          ],
          "type": "custom"
        }
      },
      "tokenizer": {
        "trigram": {
          "min_gram": 3,
          "max_gram": 5,
          "type": "nGram"
        }
      }
    }
  }
}

